WindowManager:
    Login:
    Buat:
    Konfirmasi
    Menu
    WindowUtama:
    WindowKedua:
    WindowKetiga:

<Login>:
    email : email
    password : password

    FloatLayout:
        cols:1

        FloatLayout:
            cols:
            size: root.width, root.height/2

            Button:
                background_color: (0,1,1,.5)
                text:"Selamat Datang di Pusat Informasi New Public"
                bold: True
                size_hint: 1, 0.1
                pos_hint:{"x":0,"top":0.95}
                font_size: (root.width**2 + root.height**2) / 13**4

            Button:
                background_color: (0,1,1,1)
                text:"LOGIN"
                bold: True
                size_hint : 0.3 , 0.1
                pos_hint : {"x":0.37,"top":0.8}
                font_size: (root.width**2+root.height**2)/14**4

            TextInput:
                hint_text:"Email Anda"
                pos_hint: {"x":0.31, "top":0.65}
                size_hint: 0.4, 0.1
                id: email
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                hint_text:"Password Anda"
                pos_hint: {"x":0.31, "top":0.53}
                size_hint: 0.4, 0.1
                id: password
                multiline: False
                password: True
                font_size: (root.width**2 + root.height**2) / 14**4

            Button:
                background_color : 1,1,1,1
                text:"Masuk"
                pos_hint:{"x":0.56,"y":0.3}
                size_hint: 0.15, 0.1
                font_size: (root.width**2 + root.height**2) / 15**4
                on_release:
                    root.manager.transition.direction = "left"
                    root.masuk()
            Button:
                text:"Buat Akun"
                pos_hint:{"x":0.31,"y":0.3}
                size_hint: 0.2, 0.1
                font_size: (root.width**2 + root.height**2) / 15**4
                on_release:
                    app.root.current = "buat"
                    root.manager.transition.direction = "left"

<Buat>:
    namee : namee
    email : email
    password : passw

    FloatLayout:
        cols:1

        FloatLayout:
            cols:
            size: root.width, root.height/2

            Button:
                background_color : 0,1,1,1
                text:"BUAT AKUN"
                bold: True
                size_hint : 0.3 , 0.1
                pos_hint : {"x":0.37,"top":0.9}
                font_size: (root.width**2+root.height**2)/14**4

            TextInput:
                hint_text:"Masukkan Username"
                pos_hint: {"x":0.32, "top":0.75}
                size_hint: 0.4, 0.1
                id: namee
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                hint_text:"Masukkan Alamat Email"
                pos_hint: {"x":0.32, "top":0.75-0.13}
                size_hint: 0.4, 0.1
                id: email
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                hint_text:"Masukkan Password Anda"
                pos_hint: {"x":0.32, "top":0.75-0.13*2}
                size_hint: 0.4, 0.1
                id: passw
                multiline: False
                password: True
                font_size: (root.width**2 + root.height**2) / 14**4

            Button:
                background_color: 1,0,0,1
                text:"Sudah punya akun?"
                pos_hint: {"x":0.4,"y":0.31}
                size_hint: 0.25, 0.05
                font_size: (root.width**2 + root.height**2) / 15**4
                on_release:
                    app.root.current = "login"
                    root.manager.transition.direction = "right"

            Button:
                background_color: .1,1,.2,.9
                text:"Daftar"
                pos_hint:{"x":0.32,"y":0.2}
                size_hint: 0.4, 0.1
                font_size: (root.width**2 + root.height**2) / 14**4
                on_release:
                    root.manager.transition.direction = "down"
                    root.buat()

<Konfirmasi>:
    FloatLayout:
        cols:1

        FloatLayout:
            cols:
            size: root.width, root.height/2

            Button:
                background_color: 1,.2,.2,1
                text:"Akun Telah Terdaftar"
                bold: True
                size_hint : 1 , .1
                pos_hint : {"x":0,"top":0.9}
                font_size: (root.width**2+root.height**2)/13**4

            Button:
                background_color:.5,.2,.2,1
                size_hint: 1,.5
                pos_hint: {"x":0, "y":.3}
                text: "Akun Anda Telah Terdaftar Sebagai Pengguna Layanan Public Data Base"
                font_size: (root.width**2 + root.height**2) / 9**5
            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0.27, "y":0.44}
                text: "Silahkan Login Untuk Masuk dan Menggunakan Aplikasi"
                font_size: (root.width**2 + root.height**2) / 9**5
            Button:

                text:"Masuk"
                pos_hint: {"x":0.7,"y":0.25}
                size_hint: 0.26, 0.1
                font_size: (root.width**2 + root.height**2) / 15**4
                on_release:
                    app.root.current = "login"
                    root.manager.transition.direction = "up"

<WindowUtama>:
    provinsi : provinsi
    kabupaten : kabupaten

    FloatLayout:
        cols:1
        FloatLayout:
            cols:
            size: root.width, root.height/2

            TextInput:
                hint_text: "Masukkan Kode Provinsi"
                pos_hint: {"x":0.21, "top":.65}
                size_hint: 0.6, 0.08
                id: provinsi
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                hint_text: "Masukkan Kabubaten/Kota"
                pos_hint: {"x":0.21, "top":0.55}
                size_hint: 0.6, 0.08
                id: kabupaten
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0.27, "y":0.67}
                text: "silahkan tekan tombol = untuk melihat kode setiap provinsi"
                font_size: (root.width**2 + root.height**2) / 14**4

            Button:
                pos_hint: {"x":0.66,"y":0.26}
                size_hint: 0.15, 0.1
                font_size: (root.width**2 + root.height**2) / 15**4
                text:"Telusuri"
                on_release:
                    root.manager.transition.direction = "right"
                    root.tempat()

            Button:
                background_color: 0,1,1,.5
                pos_hint: {"x":0,"y":.835}
                size_hint: 1, 0.1
                font_size: (root.width**2 + root.height**2) / 15**4
                text:"Halaman Utama"

            Button:
                background_color: 1,0,.4,1
                pos_hint: {"x":0,"y":.835}
                size_hint: .14, 0.1
                font_size: (root.width**2 + root.height**2) / 12**4
                text:"="
                on_release:
                    app.root.current = "menu"
                    root.manager.transition.direction = "up"

            Button:
                background_color: 1,0,.4,1
                pos_hint: {"x":0.85,"y":.85}
                size_hint: 0.14, 0.07
                font_size: (root.width**2 + root.height**2) / 15**4
                text:"Keluar"
                on_release:
                    app.root.current = "login"
                    root.manager.transition.direction = "right"

<Menu>:
    FloatLayout:
        cols:1

        FloatLayout:
            cols:
            size: root.width, root.height/2

            Button:
                background_color: 0.3,0.5,.4,1
                pos_hint: {"x":0,"y":0}
                size_hint: 1, 1
                font_size: (root.width**2 + root.height**2) / 15**4
            Label:
                size_hint: 0.5,0.12
                bold:True
                pos_hint: {"x":0.27, "y":.8}
                text: "Kode Provinsi"
                font_size: (root.width**2 + root.height**2) / 8**5
            Label:
                size_hint: 0,0
                pos_hint: {"x":0.15, "y":.7}
                text: "Jawa Barat = JBR"
                font_size: (root.width**2 + root.height**2) / 8.7**5
            Label:
                size_hint: 0,0
                pos_hint: {"x":0.15, "y":.35}
                text: "Jawa Timur = JTI"
                font_size: (root.width**2 + root.height**2) / 8.7**5
            Label:
                size_hint: 0,0
                pos_hint: {"x":0.5, "y":.7}
                text: "Jawa Tengah = JTE"
                font_size: (root.width**2 + root.height**2) / 8.7**5
            Label:
                size_hint: 0,0
                pos_hint: {"x":0.5, "y":.35}
                text: "Sulawesi Selatan = SLS"
                font_size: (root.width**2 + root.height**2) / 8.7**5
            Label:
                size_hint: 0,0
                pos_hint: {"x":0.85, "y":.7}
                text: "Sulawesi Barat = SLB"
                font_size: (root.width**2 + root.height**2) / 8.7**5
            Label:
                size_hint: 0,0
                pos_hint: {"x":0.85, "y":.35}
                text: "Sulawesi Tengah = SLT"
                font_size: (root.width**2 + root.height**2) / 8.7**5
            Button:
                background_color: 1,1,1,1
                pos_hint: {"x":0,"y":.835}
                size_hint: .14, 0.1
                font_size: (root.width**2 + root.height**2) /7**5
                text:"<"
                on_release:
                    app.root.current = "utama"
                    root.manager.transition.direction = "down"
<WindowKedua>:
    Button:
        text:"Next"
        on_release:
            root.manager.transition.direction = "right"
            root.list()

<WindowKetiga>:
    email : email
    tempat : tempat
    FloatLayout:
        cols:1

        FloatLayout:
            cols:
            size: root.width, root.height/2

            Label:
                pos_hint: {"x":0.13,"top":0.9}
                bold : True
                size_hint: 0.25, 0.05
                font_size: (root.width**2 + root.height**2) / 13**4
                text:"Pusat Informasi"

            Label:
                pos_hint: {"x":0,"top":0.8}
                size_hint: 1, 0.05
                font_size: (root.width**2 + root.height**2) / 15**4
                text:"Informasi terkait pusat perbelanjaan akan dikirimkan melalui email anda"

            Label:
                pos_hint: {"x":0,"top":0.75}
                size_hint: 1, 0.05
                font_size: (root.width**2 + root.height**2) / 15**4
                text:"Silahkan masukkan alamat email anda dan klik tombol kirimkan"

            Button:
                pos_hint: {"x":0.7,"top":0.35}
                size_hint: 0.25, 0.08
                font_size: (root.width**2 + root.height**2) / 15**4
                text:"Kirimkan"
                on_release:
                    root.pesan()
                    app.root.current = "konfir1"
                    root.manager.transition.direction = "up"


            Button:
                pos_hint: {"x":0.7,"y":0.17}
                size_hint: 0.25, 0.06
                font_size: (root.width**2 + root.height**2) / 15**4
                text:"Kembali"
                on_release:
                    app.root.current = "utama"
                    root.manager.transition.direction = "left"
            TextInput:
                hint_text: "Masukkan Email Tujuan"
                pos_hint: {"x":0.21, "top":.65}
                size_hint: 0.6, 0.08
                id: email
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4
            TextInput:
                hint_text: "Masukkan Nama Tempat"
                pos_hint: {"x":0.21, "top":.55}
                size_hint: 0.6, 0.08
                id: tempat
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4
